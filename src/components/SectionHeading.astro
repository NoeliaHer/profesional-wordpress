---
const { tagline, imageUrl } = Astro.props;
---

<div class="pt-16 pb-4 border-b">
  <div class="text-left my-2 px-12">
    <p class="text-2xl text-gray-800">{tagline}</p>
  </div>
  <div class="py-4 flex items-center justify-center">
    <img src={imageUrl} alt="Profesional Wordpress">
  </div>
</div>
